<!DOCTYPE html>
<html>
<head>
  <title>DataEngineering</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>

<% if signed_in? %>
    <h2 id="welcome">Welcome, <%= current_user.first_name %> [ <%= link_to "Logout", session_path, method: :delete %> ] </h2>
<% end %>


<%= yield %>

</body>
</html>
